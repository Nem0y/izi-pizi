#include <iostream>

int main(){
int num;
std::cout << "Entered number: ";
std::cin >> num;

if (num > 0 && num < 10) {
  for ( int i = 0; i < 10; i++)
    std::cout << num << " x " << i << " = " << num * i << std::endl;
}                                                                                  
std::cout << "Wrong input";

return 0;
}
